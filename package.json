{
   "name": "rehype-img-as",
   "version": "0.0.3",
   "main": "./lib/index.js",
   "description": "use alt attribute as alternative store.",
   "license": "MIT",
   "repository": "kthjm/rehype-img-as",
   "author": "kthjm <toxictoxer@gmail.com>",
   "keywords": [
      "rehype",
      "plugin"
   ],
   "prettier": {
      "tabWidth": 3,
      "semi": false,
      "singleQuote": true
   },
   "babel": {
      "presets": ["es2015", "flow"],
      "plugins": ["add-module-exports", "array-includes"],
      "env": {
         "build": {
            "plugins": ["transform-regenerator"]
         },
         "test": {
            "presets": ["power-assert"],
            "plugins": ["istanbul", "rewire"]
         }
      }
   },
   "nyc": {
      "require": ["babel-register"],
      "sourceMap": false,
      "instrument": false
   },
   "scripts": {
      "flow": "flow",
      "nycha": "cross-env NODE_ENV=test nyc mocha --recursive",
      "test": "run-s flow nycha",
      "coverage": "nyc report --reporter=text-lcov | coveralls",
      "build": "cross-env NODE_ENV=build babel ./src -d ./lib",
      "postbuild": "flow-copy-source ./src ./lib",
      "format": "prettier --write **/*.js package.json",
      "precommit": "yarn format",
      "typed": "flow-typed",
      "node": "node --require babel-register"
   },
   "devDependencies": {
      "babel-cli": "^6.26.0",
      "babel-plugin-add-module-exports": "^0.2.1",
      "babel-plugin-array-includes": "^2.0.3",
      "babel-plugin-istanbul": "^4.1.4",
      "babel-plugin-rewire": "^1.1.0",
      "babel-plugin-transform-regenerator": "^6.26.0",
      "babel-preset-es2015": "^6.24.1",
      "babel-preset-flow": "^6.23.0",
      "babel-preset-power-assert": "^1.0.0",
      "babel-register": "^6.26.0",
      "coveralls": "^2.13.1",
      "cross-env": "^5.0.5",
      "flow-bin": "^0.54.0",
      "flow-copy-source": "^1.2.1",
      "flow-typed": "^2.1.5",
      "husky": "^0.14.3",
      "mocha": "^3.5.0",
      "npm-run-all": "^4.1.1",
      "nyc": "^11.1.0",
      "power-assert": "^1.4.4",
      "prettier": "^1.6.1",
      "rehype-minify-whitespace": "^2.0.3",
      "remark": "^8.0.0",
      "remark-rehype": "^2.0.1",
      "rewire": "^2.5.2",
      "unified": "^6.1.5"
   },
   "dependencies": {
      "hast-util-select": "^1.0.1",
      "regenerator-runtime": "^0.11.0",
      "rehype": "^5.0.0"
   }
}
